{
  "name": "BitGoWalletRecoveryWizard",
  "description": "A UI-based desktop app for BitGo Recoveries",
  "version": "1.0.1",
  "private": true,
  "main": "src/main.js",
  "homepage": "./",
  "scripts": {
    "start": "node scripts/start.js",
    "build-react": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "dev": "NODE_ENV=development nf start",
    "electron": "electron .",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-clean": "rm -rf ./build/*",
    "package": "node scripts/package.js",
    "publish": "electron-forge publish --skip-package"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "bitcoinjs-lib": "^3.3.2",
    "bitgo": "^4.28.0",
    "bluebird": "^3.5.1",
    "bootstrap": "4.0.0",
    "buffer": "^5.0.8",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dns": "^0.2.2",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "electron-localshortcut": "^3.1.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.3",
    "ethereumjs-util": "^5.1.4",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prova-lib": "^0.2.9",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-mailto": "^0.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-transition-group": "^2.2.1",
    "reactstrap": "^5.0.0-beta",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sjcl": "^1.0.7",
    "style-loader": "0.19.0",
    "superagent": "^3.8.2",
    "superagent-as-promised": "^4.0.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "asar": "^0.14.3",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "electron": "^1.8.4",
    "electron-compile": "^6.4.2",
    "electron-forge": "^5.1.1",
    "electron-prebuilt-compile": "1.8.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-winstaller": "^2.6.4",
    "electron-wix-msi": "^1.3.0",
    "foreman": "^2.0.0",
    "jest": "20.0.4",
    "node-sass-chokidar": "^0.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "wallet_recovery_wizard",
        "authors": "Kevin Kennis <kevin@bitgo.com>"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "BitGo",
        "name": "wallet-recovery-wizard"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "walletrecoverywizard"
      }
    }
  }
}
