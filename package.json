{
  "name": "BitGoWalletRecoveryWizard",
  "author": "BitGo, Inc.",
  "description": "A UI-based desktop app for BitGo Recoveries",
  "version": "2.7.5",
  "private": true,
  "main": "resources/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "NODE_ENV=development concurrently 'node scripts/start.js' 'node scripts/electron-wait-react'",
    "dev": "NODE_ENV=development concurrently 'node scripts/start.js' 'node scripts/electron-wait-react'",
    "electron": "electron .",
    "build": "NODE_ENV=production node scripts/build.js",
    "pack": "npm run build && ./node_modules/.bin/electron-builder --dir",
    "lint": "eslint --ext .js src",
    "check-fmt": "prettier --check src"
  },
  "dependencies": {
    "autoprefixer": "8.6.5",
    "bignumber.js": "^9.0.0",
    "bitgo": "^11.8.0-rc.3",
    "bitgo-utxo-lib": "^1.8.0",
    "bootstrap": "^4.3.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "dns": "file:./src/mock/dns",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "1.1.11",
    "jquery": "^3.4.1",
    "jszip": "3.1.5",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.2",
    "raf": "3.4.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1",
    "react-select": "1.3.0",
    "react-transition-group": "2.5.3",
    "reactstrap": "6.5.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "ajv": "6.9.1",
    "asar": "0.14.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-preset-react-app": "3.1.2",
    "concurrently": "3.6.1",
    "css-loader": "1.0.1",
    "electron": "9.2.1",
    "electron-builder": "^22.5.1",
    "eslint": "4.10.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "^24.8.0",
    "prettier": "2.1.2",
    "react-dev-utils": "5.0.3",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "com.bitgo.WalletRecoveryWizard",
    "directories": {
      "buildResources": "resources",
      "app": "app"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg",
      "icon": "icons/mac/appicon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/win/appicon.ico"
    },
    "linux": {
      "target": "deb",
      "maintainer": "support@bitgo.com",
      "icon": "icons/png"
    }
  },
  "engines": {
    "node": ">=10.17.0",
    "npm": ">=6.11.3"
  }
}
