{
  "name": "bitgo-wallet-recovery-wizard",
  "version": "4.0.0",
  "description": "A UI-based desktop app for BitGo Recoveries",
  "main": "./.webpack/main",
  "scripts": {
    "start": "IS_DEVELOPMENT=true electron-forge start",
    "package": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" NODE_ENV=production electron-forge make",
    "extract-messages": "extract-messages -l=en,ja -o lang/ -d en --flat true 'src/**/messages.ts'",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "mocha",
    "test:debug": "mocha --inspect-brk"
  },
  "keywords": [],
  "author": "BitGo, Inc.",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icon/mac/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bitgo-wallet-recovery-wizard"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' * 'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "nodeIntegration": true,
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            },
            "port": 3002,
            "loggerPort": 9002
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@bitgo/sdk-core": "1.1.0-rc.24",
    "@bitgo/statics": "7.0.0-rc.0",
    "@bitgo/utxo-lib": "2.4.0",
    "@blueprintjs/core": "3.38.1",
    "@blueprintjs/icons": "3.24.0",
    "@blueprintjs/select": "3.15.3",
    "@lottiefiles/react-lottie-player": "^3.0.5",
    "bignumber.js": "9.0.0",
    "bip32": "^2.0.6",
    "bitgo": "14.2.0-rc.35",
    "classnames": "^2.2",
    "cryptocurrency-icons": "git+https://github.com/BitGo/cryptocurrency-icons.git#85fe13d3767decc245bd808a3134e6822c6ab3db",
    "d3": "5.15.0",
    "d3-format": "2.0.0",
    "date-fns": "2.25.0",
    "electron-rebuild": "3.2.3",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "filesize": "6.0.1",
    "formik": "2.2.6",
    "jspdf": "2.1.1",
    "jszip": "3.10.0",
    "lodash": "4.17.20",
    "pluralize": "8.0.0",
    "qrcode": "^1.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "6.0.3",
    "react-intl": "^5.13.2",
    "react-motion": "0.5.2",
    "react-router-dom": "^5.2.0",
    "react-textarea-autosize": "7.1.2",
    "react-webcam": "^5.2.3",
    "sanitize.css": "12.0.1",
    "secrets.js-grempe": "2.0.0",
    "secure-json-parse": "^2.4.0",
    "tachyons-sass": "4.9.5",
    "theme-ui": "0.9.1",
    "uuid": "^8.3.2",
    "yup": "0.32.8",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@electron-forge/cli": "v6.0.0-beta.60",
    "@electron-forge/maker-deb": "v6.0.0-beta.60",
    "@electron-forge/maker-rpm": "v6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "v6.0.0-beta.60",
    "@electron-forge/maker-zip": "v6.0.0-beta.60",
    "@electron-forge/plugin-webpack": "v6.0.0-beta.60",
    "@svgr/webpack": "5.5.0",
    "@types/classnames": "2.2.9",
    "@types/lodash": "4.14.161",
    "@types/mocha": "8.2.1",
    "@types/pluralize": "0.0.29",
    "@types/qrcode": "1.4.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-textarea-autosize": "4.3.5",
    "@types/styled-components": "5.1.0",
    "@types/theme-ui": "0.3.7",
    "@types/yup": "0.29.9",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "coffee-script": "1.12.7",
    "copy-webpack-plugin": "6.4.1",
    "cross-env": "7.0.3",
    "css-loader": "^4.3.0",
    "electron": "11.3.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-react-intl-messages": "^4.1.1",
    "filemanager-webpack-plugin": "6.1.6",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "mocha": "8.3.2",
    "node-loader": "^1.0.2",
    "node-sass": "4.14.1",
    "prettier": "^2.4.1",
    "sass-loader": "10.0.2",
    "spectron": "^13.0.0",
    "style-loader": "^1.3.0",
    "svg-url-loader": " 6.0.0",
    "typescript": "^4.1.5"
  },
  "resolutions": {
    "keccak": "^3.0.2"
  }
}
