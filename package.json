{
  "name": "BitGoWalletRecoveryWizard",
  "author": "BitGo, Inc.",
  "description": "A UI-based desktop app for BitGo Recoveries",
  "version": "1.8.0",
  "private": true,
  "main": "src/main.js",
  "homepage": "./",
  "scripts": {
    "start": "NODE_ENV=development concurrently 'node scripts/start.js' 'node scripts/electron-wait-react'",
    "dev": "NODE_ENV=development concurrently 'node scripts/start.js' 'node scripts/electron-wait-react'",
    "build-clean": "rm -rf ./build/*",
    "out-clean": "rm -rf ./out/*",
    "electron": "electron .",
    "build": "NODE_ENV=production node scripts/build.js",
    "make": "export NODE_ENV=production && node scripts/build.js && electron-forge make -p 'darwin' && electron-forge make -p 'win32'"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@ledgerhq/hw-app-btc": "4.39.0",
    "@ledgerhq/hw-transport-node-hid": "4.40.0",
    "autoprefixer": "8.6.5",
    "bitgo-utxo-lib": "1.3.0",
    "bootstrap": "4.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "electron-compile": "6.4.4",
    "file-loader": "1.1.11",
    "jquery": "3.3.1",
    "jszip": "3.1.5",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.6",
    "promise": "8.0.2",
    "raf": "3.4.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1",
    "react-select": "1.3.0",
    "react-transition-group": "2.5.3",
    "reactstrap": "6.5.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "ajv": "6.9.1",
    "asar": "0.14.6",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-preset-react-app": "3.1.2",
    "concurrently": "3.6.1",
    "css-loader": "1.0.1",
    "electron-forge": "5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "electron-squirrel-startup": "1.0.0",
    "electron-wix-msi": "1.3.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "fs-extra": "6.0.1",
    "html-webpack-plugin": "3.2.0",
    "jest": "20.0.4",
    "react-dev-utils": "5.0.3",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "asar": true,
        "icon": "./src/images/appicon"
      },
      "electronWinstallerConfig": {
        "name": "BitGoWalletRecoveryWizard",
        "setupIcon": "./src/images/appicon.ico"
      },
      "electronInstallerDMG": {
        "icon": "./src/images/appicon.icns"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "BitGoWalletRecoveryWizard"
      }
    }
  }
}
